{
  "name": "rtc-sportsbook",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server/index.ts",
    "dev:all": "concurrently \"npm run server\" \"npm run dev:sb\" \"npm run dev:bo\"",
    "dev:sb": "vite --port 5173 --config vite.sportsbook.config.ts",
    "dev:bo": "vite --port 5175 --config vite.backoffice.config.ts",
    "server": "tsx watch src/server/index.ts",
    "build": "npm run build:sb && npm run build:bo",
    "build:sb": "tsc && vite build --config vite.sportsbook.config.ts",
    "build:bo": "tsc && vite build --config vite.backoffice.config.ts",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "start": "npm run dev"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.25.0",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "express": "^4.18.3",
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.22.3",
    "socket.io": "^4.7.4",
    "socket.io-client": "^4.7.4",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.24",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.7.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2"
  }
}